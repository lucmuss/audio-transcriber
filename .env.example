# Audio Transcriber Environment Variables

# API Configuration
AUDIO_TRANSCRIBE_API_KEY=your-api-key-here
AUDIO_TRANSCRIBE_BASE_URL=https://api.openai.com/v1
AUDIO_TRANSCRIBE_MODEL=whisper-1

# Output Configuration
AUDIO_TRANSCRIBE_OUTPUT_DIR=./transcriptions
AUDIO_TRANSCRIBE_SEGMENTS_DIR=./segments
AUDIO_TRANSCRIBE_SUMMARY_DIR=./summaries
AUDIO_TRANSCRIBE_EXPORT_DIR=./exports

# Processing Configuration
AUDIO_TRANSCRIBE_SEGMENT_LENGTH=600
AUDIO_TRANSCRIBE_OVERLAP=10
AUDIO_TRANSCRIBE_CONCURRENCY=4

# Docker Configuration
RUN_TESTS=false